<?xml version="1.0" encoding="utf-8"?>
<neatparams:NeatParamsPanel xmlns:mx="http://www.adobe.com/2006/mxml"  
							xmlns:neatparams="org.brainfarm.flex.mvcs.view.neatparams.*">

	<mx:DataGrid x="0" y="0" 
				 width="250" height="100%"
				 dataProvider="{neatParams}">
		<mx:columns>
			<mx:DataGridColumn headerText="Parameter" dataField="name"/>
			<mx:DataGridColumn headerText="Value" dataField="value"/>
		</mx:columns>
	</mx:DataGrid>
</neatparams:NeatParamsPanel>
