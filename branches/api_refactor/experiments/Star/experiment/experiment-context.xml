<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">
	 
	<bean id="experiment" class="org.brainfarm.java.feat.context.Experiment">
		<property name="runs" value="1"/>
		<property name="epochs" value="${epoch}"/>
		<property name="population">
			<bean class="org.brainfarm.java.feat.Population"/>
		</property>
		<property name="populationSize" value="500"/>
		<property name="evolutionStrategy" ref="evolutionStrategy"/>
		<property name="genome" ref="genome"/>
	</bean>
	
	<bean id="evolutionStrategy" class="org.brainfarm.java.feat.EvolutionStrategy">
		<property name="organismEvaluator">
			<bean class="org.gatech.feat.experiments.star.StarOrganismEvaluator"/>
		</property>
		<property name="nodeClass" value="org.gatech.feat.experiments.star.StarNode"/>
	</bean>
	
	<import resource="genome.xml"/>
	
</beans>
